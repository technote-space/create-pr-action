{
  "name": "@technote-space/create-pr-action",
  "version": "2.0.27",
  "description": "This is a GitHub Actions that executes an arbitrary command and commits the changes to the new pull request.",
  "keywords": [
    "github",
    "github actions" 
  ],
  "homepage": "https://github.com/technote-space/create-pr-action",
  "bugs": {
    "url": "https://github.com/technote-space/create-pr-action/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/technote-space/create-pr-action.git"
  },
  "license": "MIT",
  "author": {
    "name": "Technote",
    "email": "technote.space@gmail.com",
    "url": "https://technote.space"
  },
  "files": [
    "lib",
    "action.yml"
  ],
  "scripts": {
    "build": "tsc",
    "cover": "jest --coverage",
    "lint": "eslint 'src/**/*.ts' '__tests__/**/*.ts' --cache",
    "lint:fix": "eslint --fix 'src/**/*.ts' '__tests__/**/*.ts'",
    "release": "yarn release-ga --test",
    "test": "yarn lint && yarn cover",
    "update": "npx npm-check-updates -u && yarn install && yarn upgrade && yarn audit",
    "postinstall": "[ -n \"$CI\" ] || [ ! -f node_modules/.bin/husky ] || husky install",
    "prepublishOnly": "[ -n \"$CI\" ] || [ ! -f node_modules/.bin/pinst ] || pinst --disable",
    "postpublish": "[ -n \"$CI\" ] || [ ! -f node_modules/.bin/pinst ] || pinst --enable"
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "@technote-space/github-action-helper": "^5.2.2",
    "@technote-space/github-action-pr-helper": "^2.2.3",
    "npm-check-updates": "^11.1.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.0",
    "@commitlint/config-conventional": "^12.0.0",
    "@technote-space/github-action-test-helper": "^0.7.2",
    "@technote-space/release-github-actions-cli": "^1.8.4",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "husky": "^5.1.1",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "lint-staged": "^10.5.4",
    "nock": "^13.0.8",
    "pinst": "^2.1.6",
    "ts-jest": "^26.5.2",
    "typescript": "^4.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
