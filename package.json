{
  "name": "@technote-space/create-pr-action",
  "version": "2.0.29",
  "description": "This is a GitHub Actions that executes an arbitrary command and commits the changes to the new pull request.",
  "keywords": [
    "github",
    "github actions"
  ],
  "homepage": "https://github.com/technote-space/create-pr-action",
  "bugs": {
    "url": "https://github.com/technote-space/create-pr-action/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/technote-space/create-pr-action.git"
  },
  "license": "MIT",
  "author": {
    "name": "Technote",
    "email": "technote.space@gmail.com",
    "url": "https://technote.space"
  },
  "files": [
    "lib",
    "action.yml"
  ],
  "scripts": {
    "build": "tsc",
    "cover": "jest --coverage",
    "lint": "eslint 'src/**/*.ts' '__tests__/**/*.ts' --cache",
    "lint:fix": "eslint --fix 'src/**/*.ts' '__tests__/**/*.ts'",
    "release": "yarn release-ga --test",
    "test": "yarn lint && yarn cover",
    "update": "npm_config_yes=true npx npm-check-updates -u --timeout 100000 && yarn install && yarn upgrade && yarn audit",
    "postinstall": "[ -n \"$CI\" ] || [ ! -f node_modules/.bin/husky ] || husky install",
    "prepublishOnly": "[ -n \"$CI\" ] || [ ! -f node_modules/.bin/pinst ] || pinst --disable",
    "postpublish": "[ -n \"$CI\" ] || [ ! -f node_modules/.bin/pinst ] || pinst --enable"
  },
  "dependencies": {
    "@actions/core": "^1.3.0",
    "@actions/github": "^5.0.0",
    "@technote-space/github-action-helper": "^5.2.9",
    "@technote-space/github-action-pr-helper": "^2.2.7",
    "npm-check-updates": "^11.5.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@technote-space/github-action-test-helper": "^0.7.12",
    "@technote-space/release-github-actions-cli": "^1.8.5",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "husky": "^6.0.0",
    "jest": "^27.0.1",
    "jest-circus": "^27.0.1",
    "lint-staged": "^11.0.0",
    "nock": "^13.0.11",
    "pinst": "^2.1.6",
    "ts-jest": "^27.0.0",
    "typescript": "^4.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
